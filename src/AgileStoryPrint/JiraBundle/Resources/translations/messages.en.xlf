        <body>
            <trans-unit id="head.title">
                <source>head.title</source>
                <target>
                    Jira2ScrumBoard: Printing Index Cards for Scrum Board 
                    from Atlassian Jira
                </target>
            </trans-unit>

            <trans-unit id="project.name">
                <source>project.name</source>
                <target>Jira2ScrumBoard</target>
            </trans-unit>

            <!-- Page index -->
            <trans-unit id="index.lead">
                <source>index.lead</source>
                <target><![CDATA[
                    The easier way to <strong>print</strong> your <strong>Jira 
                    stories</strong>
                    ]]>
                </target>
            </trans-unit>
            <trans-unit id="index.tagline">
                <source>index.tagline</source>
                <target><![CDATA[
                    This tools will transform your Jira stories export 
                    <em>(XML or Excel)</em> to beautiful Story cards. Upload 
                    your file below and click on "Convert" to get your stories 
                    in a printable file.
                    ]]>
                </target>
            </trans-unit>


            <!-- Form -->
            <trans-unit id="form.file.label">
                <source>form.file.label</source>
                <target>Jira export file (XML or XLS)</target>
            </trans-unit>

            <trans-unit id="form.file.button">
