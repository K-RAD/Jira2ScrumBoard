<?xml version="1.0"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file source-language="en" datatype="plaintext" original="file.ext">
        <body>
            <trans-unit id="head.title">
                <source>head.title</source>
                <target>
                    Jira2ScrumBoard: Printing Index Cards for Scrum Board 
                    from Atlassian Jira
                </target>
            </trans-unit>

            <trans-unit id="project.name">
                <source>project.name</source>
                <target>Jira2ScrumBoard</target>
            </trans-unit>

            <!-- Page index -->
            <trans-unit id="index.lead">
                <source>index.lead</source>
                <target><![CDATA[
                    The easier way to <strong>print</strong> your <strong>Jira 
                    stories</strong>
                    ]]>
                </target>
            </trans-unit>
            <trans-unit id="index.tagline">
                <source>index.tagline</source>
                <target><![CDATA[
                    Upload your Jira export file <em>(XML or XLS)</em> on the 
                    form below and click on "Convert" to get your stories in 
                    beautiful Story cards
                    ]]>
                </target>
            </trans-unit>


            <!-- Form -->
            <trans-unit id="form.file.label">
                <source>form.file.label</source>
                <target>Jira export file (XML or XLS)</target>
            </trans-unit>

            <trans-unit id="form.file.button">
                <source>form.file.button</source>
                <target>Choose file</target>
            </trans-unit>

            <trans-unit id="form.submit.button">
                <source>form.submit.button</source>
                <target>Convert</target>
            </trans-unit>

            <!-- Services -->
            <trans-unit id="services.title">
                <source>services.title</source>
                <target>How it works</target>
            </trans-unit>

            <trans-unit id="services.subtitle">
                <source>services.subtitle</source>
                <target>
                    Jira2ScrumBoard is really simple to use. Its goal is to
                    save your time when you want print your Jira Stories.
                </target>
            </trans-unit>

            <trans-unit id="services.option1.title">
                <source>services.option1.title</source>
                <target>1. Export your Jira stories</target>
            </trans-unit>
            <trans-unit id="services.option1.text">
                <source>services.option1.text</source>
                <target>
                    <![CDATA[
                    Filter in Jira stories you want to print. For example:
                    do a search as "sprint=1" to get all stories from your
                    sprint #1. Then, export them to XML or Excel (All fields).
                    ]]>
                </target>
            </trans-unit>

            <trans-unit id="services.option2.title">
                <source>services.option2.title</source>
                <target>2. Upload your file here</target>
            </trans-unit>
            <trans-unit id="services.option2.text">
                <source>services.option2.text</source>
                <target>
                    <![CDATA[
                    Upload your exported file into the form upper and
                    click on the button "Convert". Only XML or Excel file with 
                    a size below 1MB are supported.
                    ]]>
                </target>
            </trans-unit>

            <trans-unit id="services.option3.title">
                <source>services.option3.title</source>
                <target>3. Print your Index cards</target>
            </trans-unit>
            <trans-unit id="services.option3.text">
                <source>services.option3.text</source>
                <target>
                    <![CDATA[
                    When the processing will be done, you will be redirected to 
                    a congratulation page. Index cards are hidden on this page, 
                    so print this page and you will see your Index cards.
                    ]]>
                </target>
            </trans-unit>

            <!-- FAQ -->
            <trans-unit id="faq.title">
                <source>faq.title</source>
                <target>Frequently Asked Questions</target>
            </trans-unit>

            <trans-unit id="faq.subtitle">
                <source>faq.subtitle</source>
                <target>
                    Feel free to contact me if you didn't find your solution.
                </target>
            </trans-unit>

            <!-- Author -->
            <trans-unit id="author.title">
                <source>author.title</source>
                <target>Who's behind this project?</target>
            </trans-unit>

            <trans-unit id="author.subtitle">
                <source>author.subtitle</source>
                <target><![CDATA[
                    This project is a totally free and not attached to Atlassian
                    and Jira brands.<br />You can find the source code in the 
                    <a href="https://github.com/glonlas/AgileStoryPrint">Github 
                    repot</a>. Feel free to participate to this project by 
                    sending me your Pull request or fork it.
                    ]]>
                </target>
            </trans-unit>

            <trans-unit id="author.name">
                <source>author.name</source>
                <target>GÃ©rald LONLAS</target>
            </trans-unit>

            <trans-unit id="author.position">
                <source>author.position</source>
                <target>Product owner / Agile coach</target>
            </trans-unit>

            <trans-unit id="team_member.name">
                <source>team_member.name</source>
                <target>You?</target>
            </trans-unit>

            <trans-unit id="team_member.position">
                <source>team_member.position</source>
                <target>Github Contributor</target>
            </trans-unit>

            <!-- Contact -->
            <trans-unit id="contact.title">
                <source>contact.title</source>
                <target>Contact Me</target>
            </trans-unit>

            <trans-unit id="contact.subtitle">
                <source>contact.subtitle</source>
                <target>
                    You want a new feature, you found a bug or something to 
                    tell me. Please send me a message.
                </target>
            </trans-unit>

            <trans-unit id="contact.name.placeholder">
                <source>contact.name.placeholder</source>
                <target>Your Name *</target>
            </trans-unit>

            <trans-unit id="contact.email.placeholder">
                <source>contact.email.placeholder</source>
                <target>Your Email *</target>
            </trans-unit>
            
            <trans-unit id="contact.phone.placeholder">
                <source>contact.phone.placeholder</source>
                <target>Your Phone</target>
            </trans-unit>

            <trans-unit id="contact.message.placeholder">
                <source>contact.message.placeholder</source>
                <target>Your Message *</target>
            </trans-unit>

            <trans-unit id="contact.btn.submit">
                <source>contact.btn.submit</source>
                <target>Send Message</target>
            </trans-unit>

            <trans-unit id="contact.sent">
                <source>contact.sent</source>
                <target>Your message has been sent</target>
            </trans-unit>

            <!-- Complete : Downloading Story print file -->
            <trans-unit id="complete.title">
                <source>complete.title</source>
                <target>Processing complete</target>
            </trans-unit>
            <trans-unit id="complete.text">
                <source>complete.text</source>
                <target><![CDATA[
                    <a href="javascript:window.print();">Print your stories cards</a>
                    <br/><br/> <strong>Want more than one card per page?</strong>
                    <br/>Setup your printer before printing yours cards. 
                    <br/> We suggest you to print 4 cards per pages.
                    ]]>
                </target>
            </trans-unit>

            <!-- No stories generated -->
            <trans-unit id="emptyStory.title">
                <source>emptyStory.title</source>
                <target>No story found in your file</target>
            </trans-unit>
            <trans-unit id="emptyStory.text">
                <source>emptyStory.text</source>
                <target><![CDATA[
                    Sorry we didn't find any stories in your file. Please try 
                    with another file.
                    ]]></target>
            </trans-unit>

            <!-- Error : Wrong file format -->
            <trans-unit id="wrongFile.title">
                <source>wrongFile.title</source>
                <target>Wrong file uploaded</target>
            </trans-unit>
            <trans-unit id="wrongFile.text">
                <source>wrongFile.text</source>
                <target><![CDATA[
                    The file you send to us isn't a Jira XML or XLs export file.
                    Please try with another file.
                    ]]></target>
            </trans-unit>

            <!-- Footer -->
            <trans-unit id="footer.copyright">
                <source>footer.copyright</source>
                <target><![CDATA[
                    Copyright &copy; 2015 Jira2ScrumBoard
                    ]]>
                </target>
            </trans-unit>

            <!-- Story Card -->
            <trans-unit id="card.effort">
                <source>card.effort</source>
                <target>Effort:</target>
            </trans-unit>
            <trans-unit id="card.key">
                <source>card.key</source>
                <target>Story ID:</target>
            </trans-unit>
            <trans-unit id="card.type">
                <source>card.type</source>
                <target>Type:</target>
            </trans-unit>
            <trans-unit id="card.project">
                <source>card.project</source>
                <target>Project:</target>
            </trans-unit>

             <!-- Contact -->
            <trans-unit id="contact.msg.title">
                <source>contact.msg.title</source>
                <target>Message was succefully sent</target>
            </trans-unit>
            <trans-unit id="contact.msg.text">
                <source>contact.msg.text</source>
                <target><![CDATA[
                    Thank you for having took the time to send a message. We 
                    will reply as soon as possible.
                    ]]></target>
            </trans-unit>
        </body>
    </file>
</xliff>
