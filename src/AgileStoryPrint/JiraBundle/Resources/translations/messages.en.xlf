            </trans-unit>

            <trans-unit id="contact.btn.submit">
                <source>contact.btn.submit</source>
                <target>Send Message</target>
            </trans-unit>

            <trans-unit id="contact.sent">
                <source>contact.sent</source>
                <target>Your message has been sent</target>
            </trans-unit>

            <!-- Complete : Downloading Story print file -->
            <trans-unit id="complete.title">
                <source>complete.title</source>
                <target>Processing complete</target>
            </trans-unit>
            <trans-unit id="complete.text">
                <source>complete.text</source>
                <target><![CDATA[
                    Your file has been processed. 
                    <a href="javascript:window.print();">Print this page</a> to 
                    get your Index cards. <br/><br/> <strong>Want more than one 
                    card per page ?</strong> <br/>Set it up directly in your 
                    printer settings before printing yours cards. <br/> I 
                    suggest you to print 4 cards per pages.
                    ]]>
                </target>
            </trans-unit>

            <!-- No stories generated -->
            <trans-unit id="emptyStory.title">
                <source>emptyStory.title</source>
                <target>No story found in your file</target>
            </trans-unit>
            <trans-unit id="emptyStory.text">
                <source>emptyStory.text</source>
                <target><![CDATA[
                    Sorry we didn't found any stories in your file. Please try 
                    with another file.
                    ]]></target>
            </trans-unit>

            <!-- Error : Wrong file format -->
            <trans-unit id="wrongFile.title">
                <source>wrongFile.title</source>
                <target>Wrong file uploaded</target>
            </trans-unit>
            <trans-unit id="wrongFile.text">
                <source>wrongFile.text</source>
                <target><![CDATA[
                    The file you send to us isn't a Jira XML or XLs export file.
                    Please try with another file.
